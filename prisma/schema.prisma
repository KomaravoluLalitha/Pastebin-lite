generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Paste {
  id          String   @id @default(cuid())
  content     String
  slug        String   @unique
  expiresAt   DateTime?
  maxViews    Int?
  viewCount   Int      @default(0)
  createdAt   DateTime @default(now())
}
